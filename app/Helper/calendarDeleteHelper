<?php

namespace App\Helper;

use App\Models\Calendar;
use Spatie\GoogleCalendar\Event;
use Carbon\Carbon;
use DateTime;
use DateTimeZone;

class calendarDeleteHelper
{
    static function companyMailDelete($eventId)
    {
        if($eventId){
            $event = Event::find($eventId);
            $event->delete();
        }
    }
}
